{
    "name": "ecotone/symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "dgafka.mail@gmail.com"
        }
    ],
    "keywords": ["symfony","ecotone","ddd","messaging","message", "enterprise integration patterns", "eip", "distributed architecture"],
    "description": "Extends Ecotone with Symfony integration",
    "require": {
        "php": "^7.3",
        "ecotone/ecotone": "^0.3.0@dev",
        "symfony/console": "^3.3|^4.0",
        "symfony/framework-bundle": "^3.2|^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0",
        "phpunit/phpunit": "^6.2.3",
        "behat/behat": "^3.3.1",
        "behat/symfony2-extension": "^2.1",
        "phan/phan": "^1.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },

    "autoload": {
        "psr-4": {
            "Ecotone\\Symfony\\": ""
        }
    },
    "repositories": [],
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/phpunit",
            "Behat\\Bootstrap\\": "tests/Behat/Bootstrap",
            "Fixture\\": "tests/Fixture"
        }
    },
    "replace": {},
    "scripts": {},
    "conflict": {}
}
